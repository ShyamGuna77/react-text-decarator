{
  "name": "react-text-decorator",
  "version": "1.1.3",
  "description": "React wrapper around rough-notation with full React 19 and Next.js 15 compatibility",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "react19",
    "rough-notation",
    "annotation",
    "highlight",
    "nextjs",
    "nextjs15"
  ],
  "author": "ShyamGuna77",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShyamGuna77/react-text-decorator"
  },
  "homepage": "https://github.com/ShyamGuna77/react-text-decorator#readme",
  "dependencies": {
  "rough-notation": "^0.5.1"
},
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "^15.0.0"
   
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
